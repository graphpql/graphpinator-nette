{
    "source": {
        "directories": [
            "src"
        ]
    },
    "timeout": 10,
    "logs": {
        "text": "build/infection.log",
        "summary": "build/summary.log",
        "perMutator": "build/per-mutator.md"
    },
    "tmpDir": "/tmp/infection",
    "minMsi": 50,
    "minCoveredMsi": 95,
    "mutators": {
        "@default": true,
        "global-ignoreSourceCodeByRegex": [
            "\\\\assert\\(.*\\);"
        ]
    },
    "testFramework":"phpunit",
    "testFrameworkOptions": "-vvv"
}
